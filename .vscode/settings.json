{
  "cSpell.words": [
    "benrbray",
    "carbonads",
    "changesets",
    "clsx",
    "Codemirror",
    "Collab",
    "commitlint",
    "commonmark",
    "docsearch",
    "dropcursor",
    "gapcursor",
    "hardbreak",
    "inputrules",
    "joeattardi",
    "katex",
    "keymap",
    "mdast",
    "milkdown",
    "Mirone",
    "mixins",
    "nord",
    "Nuxt",
    "prosemirror",
    "Roboto",
    "selectednode",
    "textblock",
    "textbox",
    "Themable",
    "traptitech",
    "tsbuildinfo",
    "tsdoc",
    "twemoji",
    "ufffc",
    "unist",
    "vite",
    "vitejs",
    "vitest"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "npm.packageManager": "pnpm",
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/logs": true,
    "**/out/**": true,
    "**/pnpm-lock.yaml": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/*.idea": true,
    "**/.output": true,
    "**/dist/**": true,
    "**/tmp": true,
    "**/docs": true,
    "**/lib": true
  },
  "vitest.enable": true,
  "vitest.commandLine": "pnpm exec vitest"
}
